{
    "name": "plenta/jobs-matching-bundle",
    "type": "contao-bundle",
    "description": "Matching Bundle for Plenta.Jobs",
    "license": "proprietary",
    "authors": [
        {
            "name": "Plenta.io",
            "homepage": "https://plenta.io"
        }
    ],
    "version": "1.0",
    "repositories": [
        {
            "type": "path",
            "url": "packages/*"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "contao/conflicts": "@dev",
        "contao/core-bundle": "^5.3",
        "doctrine/dbal": "^3.2.0",
        "plenta/hugin-munin": "^1.0",
        "brkwsky/contao-jobs-bundle": "@dev",
        "plenta/contao-symfony-forms-helper-bundle": "*",
        "mvo/contao-group-widget": "^1.5",
        "hofff/contao-calendarfield": "dev-contao5",
        "symfony/http-client": "^6.4"
    },
    "require-dev": {
        "contao/manager-bundle": "5.3.*",
        "phpunit/phpunit": "^9.5.10",
        "contao/test-case": "^5.3",
        "symfony/phpunit-bridge": "^v6.4.9",
        "friendsofphp/php-cs-fixer": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Plenta\\ContaoJobsMatching\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Plenta\\ContaoJobsMatching\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "contao-components/installer": true,
            "contao/manager-plugin": true,
            "contao-community-alliance/composer-plugin": true,
            "php-http/discovery": true
        }
    }
}
